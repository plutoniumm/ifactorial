{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": "./.svelte-kit/tsconfig.json",
  "compilerOptions": {
    "target": "es2020",
    "ignoreDeprecations": "5.0",
    "baseUrl": ".",
    "paths": {
      "@components/*": [
        "src/components/*",
      ],
      "@components": [
        "src/components",
      ],
      "@images/*": [
        "src/images/*",
      ],
      "@images": [
        "src/images",
      ],
    },
  }
}